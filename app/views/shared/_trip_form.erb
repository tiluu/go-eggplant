<%= form_for(@trip) do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>

    <%= f.label :start_date %><br>
    <%= date_select :trip, :start_date %><br>

    <%= f.label :end_date %><br>
    <%= date_select :trip, :end_date %><br>

    <%= f.label :city %><br>
    <%= f.text_field :city %><br>

    <%= f.label :state_or_province %><br>
    <%= f.text_field :state_or_province %><br>

    <%= f.label :country %><br>
    <%= f.text_field :country %><br>

    <%= f.label :password %><br>
    <%= f.password_field :password %> 

    <p><%= f.submit "Submit" %></p>

<% end %>
